@using FluentBootstrap
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h4>@ViewBag.Message</h4>
@using (var form = Html.Bootstrap().Form().Begin())
{
    @form.Input("BookName", "Enter New Book")
    @form.Input("Author", "Enter Author")
    @form.Input("Publisher", "Published by: ")
    @form.Submit()


}
